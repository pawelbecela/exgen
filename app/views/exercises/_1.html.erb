    <% e6_tabs << @E6[note.name.to_sym] if @E6[note.name.to_sym] <= @exercise.high && @E6[note.name.to_sym] >= @exercise.low %>
  <% end %>
  
<div class="E1_tabs">
<% e1_tabs.sort![0,3].each do |sorted| %>
