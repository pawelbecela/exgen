<% e1_tabs = [] %>
  <% @exercise.notes.each do |note| %>
    <% e1_tabs << @E1[note.name.to_sym] if @E1[note.name.to_sym] <= @exercise.high && @E1[note.name.to_sym] >= @exercise.low %>
  <% end %>

  <% h2_tabs = [] %>
